{
  "manifest_version": 2,
  "name": "Snatcher",
  "version": "0.0.1",
  "description": "Grab web pages",
  "homepage_url": "https://github.com/phocks",
  "icons": {
    "48": "icons/bookmark-it.png",
    "96": "icons/bookmark-it@2x.png"
  },

  "permissions": ["bookmarks", "tabs"],

  "browser_action": {
    "default_icon": "icons/star-empty-38.png",
    "default_title": "Snatch!"
  },

  "background": {
    "scripts": ["dist/background.js"]
  }
}
